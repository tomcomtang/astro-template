{
  "name": "@edgeone/astro",
  "version": "1.0.5",
  "description": "Deploy your Astro site to EdgeOne Pages with automatic dependency optimization",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "astro",
    "withastro",
    "astro-adapter",
    "astro-integration",
    "adapter",
    "edgeone",
    "pages",
    "ssr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TencentEdgeOne/astro-adapter"
  },
  "homepage": "https://github.com/TencentEdgeOne/astro-adapter",
  "dependencies": {
    "@vercel/nft": "^0.27.7"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
